{
  "version": 2,
  "builds": [
    {
      "src": "webhook-handler.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "webhook-handler.js"
    },
    {
      "src": "/status",
      "dest": "webhook-handler.js"
    },
    {
      "src": "/health",
      "dest": "webhook-handler.js"
    },
    {
      "src": "/api/licenses/qa-architect.json",
      "dest": "webhook-handler.js"
    },
    {
      "src": "/legitimate-licenses.json",
      "dest": "webhook-handler.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "ignoreCommand": "if [ \"$VERCEL_GIT_COMMIT_AUTHOR_LOGIN\" = \"dependabot[bot]\" ]; then exit 0; fi; exit 1"
}
